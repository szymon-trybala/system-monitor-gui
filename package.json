{
  "name": "system-monitor",
  "version": "0.0.1",
  "description": "App showing system info",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --colors",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
    "build:win": "electron-builder build --win --x64"
  },
  "build": {
    "appId": "com.devtography.electron_boilerplate",
    "productName": "System Monitor",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "win": {
      "target": "nsis",
      "asar": false
    },
    "extraFiles" :{
      "from": "system_monitor_cli.exe",
      "to": "system_monitor_cli.exe",
      "filter": ["**/*"]
    },
    "buildVersion": "1.0.0"
  },
  "author": "XXX",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^16.9.34",
    "@types/react-burger-menu": "^2.6.1",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "chat.js": "^1.0.2",
    "react": "^16.13.1",
    "react-burger-menu": "^2.6.13",
    "react-dom": "^16.13.1",
    "react-google-charts": "2",
    "react-preloader-icon": "^0.4.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/react-router": "^5.1.5",
    "@types/react-router-dom": "^5.1.3",
    "acorn": "^7.1.1",
    "ajv": "^6.12.0",
    "copy-pkg-json-webpack-plugin": "0.0.38",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "electron": "^11.5.0",
    "electron-builder": "^22.4.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "lodash": "^4.17.15",
    "mocha": "^7.1.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "spectron": "^10.0.1",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szymon-trybala/system-monitor-gui"
  }
}
